<dialog title="Publish Files Command" buttons="accept,cancel">
	<grid>
		<columns>
			<column/>
			<column/>			
		</columns>
		<rows>
			<row>
				<flash width="511"  src="Publish Files.swf"  height="200" />
				<property id="Path" value="" />
				<property id="ExportPath" value="" />
				<property id="PublishProfilePath" value="" />
				<property id="subFolder" value="false" />
				<property id="SaveFile" value="false" />
				<property id="PublishProfile" value="false" />
			</row>	
		</rows>
	</grid>
</dialog>